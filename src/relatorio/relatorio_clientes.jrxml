	<queryString language="SQL">
		<![CDATA[select 
   iif(OSCLIENT is null or OSCLIENT = '','00000',OSCLIENT) as codigo_cliente,
   OSORCAOS NUMERO_VENDA,
   iif(OSNOMCLI is null or OSCLIENT = '' , 'Não identificado',OSNOMCLI) as cliente, 
   osvlrorc as total,
   OSLIQUID as data_ultima_venda,
   iif(CLTELEFO is null,'',CLTELEFO) as CLTELEFO 
from 
  sosa01 
inner join 
  sosa09 on(CDORCAOS=OSORCAOS)
left outer join
  crea01 on(CLCODIGO=OSCLIENT)
where
  CDTIPCAR='PIX']]>
	</queryString>
